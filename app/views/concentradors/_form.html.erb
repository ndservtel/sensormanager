<%= semantic_form_for @concentrador do |f| %>
  <%= f.inputs do %>
    <%= f.input :categoria_equipos %>
    <%= f.input :nombre %>
    <%= f.input :descripcion %>
    <%= f.input :estado %>
    <%= f.input :gis_latitud %>
    <%= f.input :float %>
    <%= f.input :gis_longitud %>
    <%= f.input :mac_address %>
    <%= f.input :numero_serie %>
    <%= f.input :ip_address %>
    <%= f.input :ip_port %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
